
<div class="center-text">
	 

  <h1><strong>Login!</strong>
</h1>

<%= form_tag login_form_path do %>

<div class="form-group">
Email: 
<br>
<%= text_field_tag :email %>
</div>

<div class="form-group">
Password:
<br>
 <%= password_field_tag :password %>
</div>

<div class="form-group">
<%= submit_tag "Login" %>
</div>

<% end %>
</div>
